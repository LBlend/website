{
  "overrides": [
    {
      "files": "*.astro",
      "options": {
        "parser": "astro"
      }
    }
  ],
  "printWidth": 120,
  "tabWidth": 2,
  "useTabs": false,
  "plugins": [
    "prettier-plugin-astro",
    "prettier-plugin-multiline-arrays",
    "prettier-plugin-organize-attributes",
    "prettier-plugin-organize-imports",
    "prettier-plugin-tailwindcss"
  ]
}
